<h1>Listings Articles</h1>
<%= link_to 'New article', new_article_path %>
<table>
	<tr>
		<th>Id</th>
		<th>Title</th>
		<th>text</th>
		<th>Posted</th>
	</tr>

	<% @articles.each do |article| %>
	<tr>
		<td><%= article.id %></td>
		<td><%= article.title %></td>
		<td><%= article.text %></td>
		<td><%= article.created_at %></td>
	</tr>
	<% end %>
</table>